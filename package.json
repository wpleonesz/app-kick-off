{
  "name": "app-kick-off-ionic-login",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 3001",
    "build": "vite build",
    "preview": "vite preview",
    "cap:init": "npx cap init app-kick-off com.example.appkickoff --web-dir=dist || true",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "android": "vite build && npx cap sync android && npx cap run android",
    "android:sync": "vite build && npx cap sync android",
    "android:build": "cd android && ./gradlew build",
    "android:build:release": "cd android && ./gradlew bundleRelease",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "android:dev": "CAPACITOR_CONFIG=capacitor.config.dev.ts npx cap run android",
    "ios": "vite build && npx cap sync ios && npx cap run ios",
    "ios:open": "npx cap open ios",
    "ios:run": "npx cap run ios",
    "ios:dev": "CAPACITOR_CONFIG=capacitor.config.dev.ts npx cap run ios"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.4.1",
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capawesome/capacitor-live-update": "^8.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@ionic/storage": "^4.0.0",
    "@tanstack/react-query": "^5.90.18",
    "@types/react-router-dom": "5",
    "history": "^5.3.0",
    "ionicons": "^8.0.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.71.1",
    "react-router": "5",
    "react-router-dom": "5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
